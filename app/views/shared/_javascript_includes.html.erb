<%= javascript_include_tag :defaults, 'basic', 'message', 'window_manager', 'chatting_system', 'conference', 'contact_request', 'conference_notifier', 'contact_list', 'channel', 'DD_roundies_0.0.2a', :cache => "cache/all-minified" %>

<script>
  <%= yield :javascripts %>
  
  <!-- --------- Rounded Corners ------------- -->
	  /* IE only */
	  //DD_roundies.addRule('.roundify', '10px');
	
	  /* varying radii, IE only */
	  //DD_roundies.addRule('.something_else', '10px 4px');
	
	  /* varying radii, "all" browsers */
	  DD_roundies.addRule('.border-radius5', '5px', true);
	  DD_roundies.addRule('.border-radius-top5', '5px 5px 0px 0px', true);
	  DD_roundies.addRule('.border-radius10', '10px', true);
	  DD_roundies.addRule('.border-radius-top10', '10px 10px 0px 0px', true);
  <!--  ------------------------------------   -->
  
  var enabledElements = document.getElementsByClassName('enabled');
  for(var i=0; i<enabledElements.length; i++)
  {
  	enabledElements[i].removeAttribute('disabled');
  }
  
  window.onload = function() {
  	<%= yield :body_onload %>
  }
</script>